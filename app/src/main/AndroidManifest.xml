<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.flyingcircus.swabber">

    <application
            android:allowBackup="true"
            android:icon="@drawable/iconplaystore"
            android:label="@string/app_name"
            android:roundIcon="@drawable/iconplaystore"
            android:supportsRtl="false"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".DatabaseTest">
        </activity>
        <activity android:name=".WinScreen">
        </activity>
        <activity android:name=".LoseScreen">
        </activity>
        <activity android:name=".GameActivity">
        </activity>
        <activity android:name=".HomeScreen">
        </activity>
        <activity android:name=".HomeScreenAnimation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <meta-data android:name="preloaded_fonts" android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>