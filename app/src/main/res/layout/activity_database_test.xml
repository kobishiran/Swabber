<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DatabaseTest">

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/highScore1"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Score" android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.24000001"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/highScore2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            tools:text="Score" android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintTop_toBottomOf="@+id/highScore1"
            android:layout_marginTop="40dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/highScore3"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            tools:text="Score" android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintTop_toBottomOf="@+id/highScore2"
            android:layout_marginTop="40dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/highScore4"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            tools:text="Score" android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintTop_toBottomOf="@+id/highScore3"
            android:layout_marginTop="40dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/highScore5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            tools:text="Score" android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.501" app:layout_constraintTop_toBottomOf="@+id/highScore4"
            android:layout_marginTop="40dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>